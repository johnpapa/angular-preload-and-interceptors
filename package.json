{
  "name": "angular-preload",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start-ng": "ng serve --proxy-config proxy.conf.json --port 7626 --open",
    "start": "WWW=dist/angular-preload node server.js",
    "build": "ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "map": "source-map-explorer dist/angular-preload/*es2015*.js*",
    "full-stack": "concurrently \"npm run backend\" \"npm run start-ng\"",
    "backend": "json-server-auth --watch db.json --routes routes.json --port 7627 --delay 1000 --middlewares ./node_modules/json-server-reset"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.2.4",
    "@angular/common": "^13.2.4",
    "@angular/compiler": "^13.2.4",
    "@angular/core": "^13.2.4",
    "@angular/forms": "^13.2.4",
    "@angular/platform-browser": "^13.2.4",
    "@angular/platform-browser-dynamic": "^13.2.4",
    "@angular/router": "^13.2.4",
    "@ngrx/data": "^13.0.2",
    "@ngrx/effects": "^13.0.2",
    "@ngrx/entity": "^13.0.2",
    "@ngrx/store": "^13.0.2",
    "@ngrx/store-devtools": "^13.0.2",
    "bulma": "^0.9.2",
    "core-js": "^3.10.0",
    "express": "^4.16.4",
    "font-awesome": "^4.7.0",
    "rxjs": "~6.6.7",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.2.5",
    "@angular/cli": "^13.2.5",
    "@angular/compiler-cli": "^13.2.4",
    "@angular/language-service": "^13.2.4",
    "@types/jasmine": "~3.6.9",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^14.14.37",
    "codelyzer": "^6.0.0",
    "concurrently": "^6.0.0",
    "json-server-auth": "^2.0.2",
    "json-server-reset": "^1.3.0",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "protractor": "~7.0.0",
    "source-map-explorer": "^2.5.2",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.5.5"
  }
}